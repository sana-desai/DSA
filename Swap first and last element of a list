# Swap function
def swapList(newList):
    newList[0], newList[-1] = newList[-1], newList[0]
    return newList
# Driver code
newList = [12, 35, 9, 56, 24]
print(swapList(newList))


# second solution
def swapList(list):
     
    first = list.pop(0)  
    last = list.pop(-1)
     
    list.insert(0, last) 
    list.append(first)  
     
    return list
     
# Driver code
newList = [12, 35, 9, 56, 24]
 
print(swapList(newList))
